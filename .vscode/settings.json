{
  "java.configuration.updateBuildConfiguration": "automatic",
  
  "files.exclude": {
    "**/.classpath": false,
    "**/.project": false,
    "**/.settings": false,
    "**/.factorypath": false,
    "**/.dccache": false,
    "README.md": false,
  },

  "workspaceCacheClean.silent": false,
  "workspaceCacheClean.autoReload": false,
  "sqltools.connections": [
    {
      "mysqlOptions": {
        "authProtocol": "default"
      },
      "previewLimit": 50,
      "server": "127.0.0.1",
      "port": 27017,
      "askForPassword": true,
      "driver": "MariaDB",
      "database": "test",
      "username": "test.users",
      "name": "mongo"
    }
  ]
}